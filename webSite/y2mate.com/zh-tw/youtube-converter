<script type="ec1f5c5e03de7701619191a3-text/javascript">
    function analyze()
    {
        var $form = $("#search-form"),
            query = $form.find("input[name='query']").val(),
            url = '/zh-tw/converter/ajax';

        if(query.trim() == "")
            return;

        var clear_pager = true;

        $.ajax({
            type: "POST",
            url: url,
            data: {
                url:query,
                ajax:1
            },
            beforeSend : function(){
                $("#loading_img").css('display','inline');
                $("#result").empty();
            },
            success: function(rpdata){

                if(rpdata.status == 'success'){
                    $("#loading_img").css('display','none');
                    $("#result").empty().append(rpdata.result);
                }
                else {
                    setTimeout(function() { window.location.reload(); }, 3000);
                }

                if (video_service != 'youtube') $('.ads_banner').empty();
                else {
                    if ($('.ads_banner').is (':empty')) {
                        /*$('.ads_banner').load ('/adsense_code');*/
                    }
                }

                if (video_extractor) {
                    if(video_extractor=='search' && video_id.indexOf('/')>-1){
                        video_id = video_id.replace(/\//g, '!!slash!!');
                    }
                    var wLocation = '/zh-tw/youtube-converter/';
                    if(video_extractor=='youtube')
                        wLocation = wLocation + encodeURIComponent(video_id);
                    else
                        wLocation = wLocation + video_extractor +'/' + encodeURIComponent(video_id);

                    if(video_extractor=='channel' || video_extractor=='user'){
                                            }

                    window.history.replaceState (null, null, wLocation);
                    $('#menu-lang a').each(function () {
                        var url =  '/_hl_/youtube-converter/';
                        if(video_extractor=='youtube'){
                            url = url.replace('_hl_', $(this).data('lang')) +  video_id;
                        } else
                            url = url.replace('_hl_', $(this).data('lang')) + video_extractor +'/' + video_id;

                        if(video_extractor=='channel' || video_extractor=='user'){
                                                    }
                        $(this).attr ('href',url);
                    });

                }
            }
        });
    }

    $( document ).ready(function() {
        $('#txt-url').keypress(function (e) {
            if (e.which == 13) {
                e.preventDefault();
                analyze();
                return false;
            }
        });

            });

    function clear_input_search(){
        $('#txt-url').val('').focus().tooltip('hide');
    }

</script>
